server.port=8889
server.servlet.context-path=/h2
###################################################### JDBC DATASOURCE
spring.datasource.driver-class-name=org.h2.Driver
spring.datasource.schema=classpath:db/schema-h2.sql
spring.datasource.data=classpath:db/data-h2.sql
spring.datasource.url=jdbc:h2:file:./db/h2-demo;AUTO_SERVER=TRUE;DB_CLOSE_DELAY=-1
spring.datasource.username=root
spring.datasource.password=test
spring.h2.console.enabled=true
logging.level.com.example.htwo=debug